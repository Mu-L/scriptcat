{
  "manifest_version": 3,
  "name": "__MSG_scriptcat__",
  "version": "0.17.0.1010",
  "author": "CodFrm",
  "description": "__MSG_scriptcat_description__",
  "options_ui": {
    "page": "src/options.html",
    "open_in_tab": true
  },
  "default_locale": "zh_CN",
  "icons": {
    "128": "assets/logo.png"
  },
  "action": {
    "default_popup": "src/popup.html"
  },
  "background": {
    "service_worker": "src/service_worker.js"
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "src/content.js"
      ],
      "run_at": "document_start",
      "all_frames": true
    }
  ],
  "permissions": [
    "tabs",
    "cookies",
    "storage",
    "activeTab",
    "scripting",
    "offscreen",
    "downloads",
    "contextMenus",
    "notifications",
    "clipboardWrite",
    "declarativeNetRequest"
  ],
  "host_permissions": [
    "<all_urls>"
  ],
  "sandbox": {
    "pages": [
      "src/sandbox.html"
    ]
  }
}